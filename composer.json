{
  "name": "ssntpl/cloud-storage",
  "description": "Laravel Storage Driver to sync files on to the multiple disks asynchronously",
  "keywords": [
    "ssntpl",
    "laravel",
    "flysystem",
    "cloud-storage"
  ],
  "homepage": "https://github.com/ssntpl/cloud-storage",
  "license": "MIT",
  "authors": [
    {
      "name": "Abhishek Sharma",
      "email": "abhishek.sharma@ssntpl.in",
      "homepage": "https://ssntpl.com",
      "role": "Developer"
    },
    {
      "name": "Sambhav Aggarwal",
      "homepage": "https://ssntpl.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "nunomaduro/collision": "^6.0",
    "larastan/larastan": "^2.0.1",
    "pestphp/pest-plugin-laravel": "^1.1",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "laravel/pint": "^1.17"
  },
  "autoload": {
    "psr-4": {
      "Ssntpl\\CloudStorage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ssntpl\\CloudStorage\\CloudStorageServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  }
}
